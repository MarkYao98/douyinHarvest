<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.markyao.mapper.CommentUserMapper">

    <insert id="insertForId" parameterType="CommentUser" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO harvest_comment_user (id,uid,sec_uid,avatar,region,language,nickname,user_age)
        VALUES
            (#{id}, #{uid}, #{secUid}, #{avatar}, #{region}, #{language}, #{nickname}, #{userAge})
    </insert>



</mapper>